<UserControl x:Class="tictactoe.Xes"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">
    <Grid>
        <Canvas x:Name="canvas1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="#f0f0f0" Margin="0">
            <Line X1="0" Y1="0" X2="100" Y2="100" Stroke="Red" StrokeThickness="10"/>
            <Line X1="0" Y1="100" X2="100" Y2="0" Stroke="Red" StrokeThickness="5"/>
        </Canvas>
    </Grid>
</UserControl>
